<div>
<p>The table and Angular form below both rendered with EJS!</p>
<table>
    <thead>
        <tr><th>Country</th><th>Million Metric Tons</th></tr>
    </thead>
    <tbody>
        <% for (var i = 0; i < soybean.length; i++) { %>
        <tr>
            <td><%= soybean[i].country %></td>
            <td><%= soybean[i].mmt %></td>
        </tr>
        <% } %>
    </tbody>
</table>

<form class="form-signin" ng-submit="onSubmit()">
    <h2 class="form-signin-heading">Please sign in</h2>
    <label for="inputEmail" class="sr-only">Email address</label>
    <input type="email" id="inputEmail" class="form-control" placeholder="Email address" ng-model="formData.email" required autofocus>
    <label for="inputPassword" class="sr-only">Password</label>
    <input type="password" id="inputPassword" class="form-control" placeholder="Password" ng-model="formData.password" required>
    <button class="btn btn-lg btn-primary btn-block" type="submit">Sign in</button>
</form>
</div>